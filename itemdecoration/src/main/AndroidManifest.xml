<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tuacy.itemdecoration">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".space.ItemDecorationSpaceActivity"
            android:label="设置间距"/>

        <activity android:name=".divider.ItemDecorationDividerActivity"
            android:label="分割线"/>

        <activity android:name=".sectionpin.ItemDecorationSectionPinActivity"
            android:label="固定标题栏"/>
    </application>

</manifest>