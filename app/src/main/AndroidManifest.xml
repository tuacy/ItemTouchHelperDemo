<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tuacy.itemtouchhelperdemo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".swipe.SwipeDeleteActivity"
            android:label="滑动删除"/>

        <activity android:name=".drag.dragsimple.DragSimpleActivity"
            android:label="drag滑动变换位置实例-1"/>

        <activity android:name=".drag.dragcomplex.DragComplexActivity"
            android:label="drag滑动变换位置实例-2"/>
    </application>

</manifest>